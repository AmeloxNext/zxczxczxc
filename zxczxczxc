    local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/kigredns/guiformyfriend/refs/heads/main/redversion.lua"))()
    local Window = OrionLib:MakeWindow({Name = "ColdTeam X", HidePremium = false, SaveConfig = true, IntroEnabled = false})
    

	    Tib = Window:MakeTab({
        Name = "ColdTeam X Main",
        Icon = "rbxassetid://7733960981",
        PremiumOnly = false
    })


Tib:AddButton({
    Name = "попу мыть",
    Callback = function(value)
        local player = game.Players.LocalPlayer
        local tool = player.Backpack["Paper"]
        tool.GripPos = Vector3.new(-0.6,1.6,1.9)
        wait(.1)
        tool.Parent = player.Character
end
})

